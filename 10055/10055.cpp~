#include<iostream>
#include<string>
#include<sstream>
using namespace std;

int diff(int,int);

int main (void){

  istringstream sstr;
  string str;

  int val1 , val2;
  
  getline(cin , str);
  
  while (cin.good() && sstr.good()){
  
    sstr.str(str);
    sstr >> val1 >> val2;
    
    cout << diff(val1, val2);

    sstr.clear();
    getline(cin , str);
  }
  return 0;
}

int diff(int val1 , int val2){

  if (val1 <= val2)
    return val2 - val1;

  return val1 - val2;

}















