#include <iostream>
#include <string>
#include <stringstream>
using namespace std;

int main(void)
{
  string str;
  istringstream sstr;
  
  getline(cin , str);
  sstr.str(str);
  
  int dna[10];
  for (int counter = 0 ; counter < 10 ; counter ++)
    sstr >> dna[counter];

  int dishes[42];
  for (int counter = 0 ; counter < 42 ; counter++)
    dishes[counter] = 0;
  
  dishes[19] = 1;

  for (int counter = 0 ; counter < 50 ; counter++)
    for (int index = 1 ; index < 41 ; index ++){
	dishes[index] = dna[dishes[index - 1] + dishes[index] + dishes[index + 1]];
	printContent(dishes[index])
    }


  
  return 0;
}

void printContent(int value){
  if (value == 0)
    cout << ' ';
  else if (value == 1)
    cout << '.';
  else if (value == 2)
    cout << 'x';
  else if (value == 3)
    cout << 'w';
}
