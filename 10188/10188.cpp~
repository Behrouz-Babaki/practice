#include <iostream>
#include <sstream>
#include <string>
using std::cout;
using std::endl;
using std::cin;
using std::getline;
using std::stringstream;
using std::string;

int main (void){

  string line;
  getline (cin , line);
  
  stringstream ss (line);
  int val;
  ss >> val;

  while (val != 0)
    {

      getline(cin, line);
      ss.clear();
      ss.str(line);
      ss >> val;
    }

  return 0;
}
